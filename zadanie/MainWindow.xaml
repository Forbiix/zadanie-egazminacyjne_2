<Window x:Class="zadanie.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zadanie"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400">
    <Grid>
        <StackPanel >
            <Button Height="75" Margin="0,20,0,0" Click="styczen_2023_Click">
                <Label FontSize="20" Content="Styczeń 2023"></Label>
            </Button>
            <Button Height="75" Margin="0,20,0,0" Click="stycznen_2024_Click">
                <Label FontSize="20" Content="Styczeń 2024"></Label>
            </Button>
            <Button Height="75" Margin="0,20,0,0" Click="czerwiec_2023_Click">
                <Label FontSize="20" Content="Czerwiec 2023"></Label>
            </Button>
            <Button Height="75" Margin="0,20,0,0" Click="czwarty_plik_Click">
                <Label FontSize="20" Content="Czwarty plik"></Label>
            </Button>
        </StackPanel>
    </Grid>
</Window>
